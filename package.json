{
  "name": "abstract-level",
  "version": "0.0.1",
  "description": "Abstract prototype for a lexicographically sorted key-value database",
  "license": "MIT",
  "main": "index.js",
  "browser": {
    "./lib/next-tick.js": "./lib/next-tick-browser.js"
  },
  "types": "./index.d.ts",
  "scripts": {
    "test": "ts-standard && tsc && standard && hallmark && nyc node test/self.js",
    "test-browsers": "airtap --coverage test/self.js",
    "test-browser-local": "airtap --coverage -p local --live test/self.js",
    "coverage": "nyc report -r lcovonly",
    "hallmark": "hallmark fix",
    "dependency-check": "dependency-check --no-dev -i buffer -i queue-microtask . test/*.js",
    "prepublishOnly": "npm run dependency-check"
  },
  "files": [
    "abstract-chained-batch.js",
    "abstract-iterator.js",
    "abstract-level.js",
    "index.js",
    "index.d.ts",
    "lib",
    "test",
    "types",
    "CHANGELOG.md",
    "UPGRADING.md",
    "header.svg",
    "sauce-labs.svg"
  ],
  "dependencies": {
    "buffer": "^6.0.3",
    "catering": "^2.1.0",
    "is-buffer": "^2.0.5",
    "level-concat-iterator": "^3.1.0",
    "level-supports": "^3.0.0",
    "level-transcoder": "^1.0.0",
    "module-error": "^1.0.1",
    "queue-microtask": "^1.2.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@voxpelli/tsconfig": "^3.0.0",
    "airtap": "^4.0.3",
    "airtap-sauce": "^1.1.0",
    "dependency-check": "^4.1.0",
    "hallmark": "^4.0.0",
    "nyc": "^15.1.0",
    "sinon": "^12.0.1",
    "standard": "^16.0.4",
    "tape": "^5.0.1",
    "ts-standard": "^11.0.0",
    "typescript": "^4.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Level/abstract-level.git"
  },
  "homepage": "https://github.com/Level/abstract-level",
  "keywords": [
    "abstract-level",
    "level",
    "leveldb"
  ],
  "engines": {
    "node": ">=12"
  }
}
